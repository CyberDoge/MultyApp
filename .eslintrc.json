{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true
  },
  "settings": {
    "react": {
      "version": "detect"
    }
  },
  "ignorePatterns": ["config/**", "scripts/**"],
  "extends": [
    "plugin:react/jsx-runtime",
    "eslint:recommended",
    "plugin:react/recommended",
    "prettier"
  ],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "plugins": ["react"],
  "rules": {
    "no-unused-vars": 0,
    "react/prop-types": 0,
    "indent": [
      "error",
      2,
      {
        "SwitchCase": 1
      }
    ]
  }
}
